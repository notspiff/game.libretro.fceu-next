<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.fceumm"
		name="NES / Famicom (FCEUmm)"
		version="1.0.0"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.nes" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>NES</platforms>
		<extensions>fds|nes|unf|unif</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<supports_keyboard>false</supports_keyboard>
		<supports_mouse>false</supports_mouse>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">NES / Famicom (FCEUmm) git</summary>
		<description>Port of FCEUmm to Libretro. FCEUmm emulates NES, Famicom, and Famicom Disk System (FDS). FCEUmm merges elements from FCEU Ultra, FCEU rerecording, FCEUXD, FCEUXDSP, FCEUXDSP CE, and FCEU-mm into a single branch of FCEU.

Icon credit: Anarkhya (anarkhya.deviantart.com)

Supported files: .fds, .nes, .unf, .unif</description>
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<assets>
			<icon>resources/icon.png</icon>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
	</extension>
</addon>
